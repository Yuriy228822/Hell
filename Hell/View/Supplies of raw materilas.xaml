<Window x:Class="Hell.View.Supplies_of_raw_materilas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hell.View"
        mc:Ignorable="d"
        Title="Поставки Сырья" Height="450" Width="400">
    <Grid>
        <StackPanel>
            <TextBlock Text="Дата поставки" Margin="5"/>
            <DatePicker x:Name="ДатаПоставкиDatePicker" Margin="5"/>

            <TextBlock Text="Количество" Margin="5"/>
            <TextBox x:Name="КоличествоTextBox" Margin="5"/>

            <TextBlock Text="Поставщик" Margin="5"/>
            <TextBox x:Name="ПоставщикTextBox" Margin="5"/>

            <TextBlock Text="Тип сырья" Margin="5"/>
            <TextBox x:Name="ТипСырьяTextBox" Margin="5"/>

            <Button Content="Сохранить" Margin="5" Click="SaveButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
